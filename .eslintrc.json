{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "arrow-body-style": ["warn", "never"],
        "max-len": ["warn", 130],
        "space-before-function-paren": ["error", "always"],    "comma-dangle": ["error", {
            "arrays": "always-multiline",
            "objects": "always-multiline",
            "imports": "always-multiline",
            "exports": "always-multiline",
            "functions": "never"
          }]
    }
}